{
  "name": "Акции Отелей",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "coral-dev": "parcel ./coral.ru/src/index.pug --no-source-maps --open --port 3000",
    "coral-build": "parcel build ./coral.ru/src/index.pug --no-source-maps --no-cache --dist-dir=@CMC/coral",
    "sunmar-dev": "parcel ./sunmar.ru/src/index.pug --no-source-maps --open --port 3000",
    "sunmar-build": "parcel build ./sunmar.ru/src/index.pug --no-source-maps --no-cache --dist-dir=@CMC/sunmar"
  },
  "devDependencies": {
    "@parcel/transformer-less": "^2.9.3",
    "@parcel/transformer-pug": "^2.9.3",
    "cssnano": "^6.0.1",
    "parcel": "latest",
    "parcel-namer-custom": "^0.2.0",
    "parcel-namer-hashless": "^1.0.5",
    "parcel-namer-rewrite": "^2.0.0-rc.3",
    "parcel-plugin-clean-easy-unsafe": "^1.0.2",
    "postcss": "^8.4.30",
    "pug": "^3.0.2"
  },
  "parcelCleanPaths": [
    "dist",
    "@CMC/*.*",
    ".parcel-cache"
  ],
  "parcel-namer-hashless": {
    "include": [
      ".js$",
      ".css$",
      ".png$",
      ".jpg$"
    ],
    "mode": "all"
  },
  "parcel-namer-custom": {
    "index.js$": "[folder]/[name].[ext]",
    "style.css$": "[folder]/[name].[hash].[type]"
  }
}
